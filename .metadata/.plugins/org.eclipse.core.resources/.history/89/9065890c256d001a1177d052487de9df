
public class sum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
    	   int a = 24;
    	   int b = 26;
    	   int c = a+b;
    	   System.out.println("The sum is: "+ c);
    	   System.out.println("The sum is:" +(a+b));
    	   
    	   multiply(3,4);
    	   
    	  
	}
	public static int multiply(int a, int b) {
		return b*a;
	   }
}
